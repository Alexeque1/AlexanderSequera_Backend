<div>
    <div class="manageUsersBack">
        <a href="http://localhost:8080/manageusers">
            <svg xmlns="http://www.w3.org/2000/svg" width="60" height="60" viewBox="0 0 24 24">
                <g fill="none">
                    <path
                        d="M24 0v24H0V0zM12.593 23.258l-.011.002l-.071.035l-.02.004l-.014-.004l-.071-.035c-.01-.004-.019-.001-.024.005l-.004.01l-.017.428l.005.02l.01.013l.104.074l.015.004l.012-.004l.104-.074l.012-.016l.004-.017l-.017-.427c-.002-.01-.009-.017-.017-.018m.265-.113l-.013.002l-.185.093l-.01.01l-.003.011l.018.43l.005.012l.008.007l.201.093c.012.004.023 0 .029-.008l.004-.014l-.034-.614c-.003-.012-.01-.02-.02-.022m-.715.002a.023.023 0 0 0-.027.006l-.006.014l-.034.614c0 .012.007.02.017.024l.015-.002l.201-.093l.01-.008l.004-.011l.017-.43l-.003-.012l-.01-.01z" />
                    <path fill="black"
                        d="M7.16 10.972A7 7 0 0 1 19.5 15.5a1.5 1.5 0 1 0 3 0c0-5.523-4.477-10-10-10a9.973 9.973 0 0 0-7.418 3.295L4.735 6.83a1.5 1.5 0 1 0-2.954.52l1.042 5.91c.069.391.29.74.617.968c.403.282.934.345 1.385.202l5.644-.996a1.5 1.5 0 1 0-.52-2.954l-2.788.491Z" />
                </g>
            </svg>
        </a>
    </div>
    {{#each userProfileResult}}
    <div class="profileFullBox" data-userprofile-id="{{_id}}">
        <div class="profileBox_header">
            <div class="profileBox_header_photoBox">
                <picture class="profileBox_header_photo">
                    <img src={{profilePhoto}} alt="" />
                </picture>
            </div>
            <div class="profileBox_header_identification">
                <div class="profileBox_nameBox">
                    <h2 class="profileBox_name"> {{first_name}} {{last_name}} </h2>
                </div>
                <div class="profileBox_accountTypeBox">
                    <p class="profileBox_accountType"> Cuenta tipo {{role}} </p>
                </div>
                <div class="profileBox_buttonsBox">
                    <div class="profileBox_buttons">
                        <button class="btn" id="toChangeRoleProfile">
                            Cambiar role
                        </button>
                    </div>
                </div>
            </div>
        </div>

        <!-- UPLOAD DODUCMENTS -->
        <div class="profileBox_data">
            <div class="profileBox_data_uploaded">
                <div class="profileBox_data_uploadedTitle">
                    <h3>Archivos subidos</h3>
                </div>
                <div class="profileBox_data_uploaded_verify">
                    <p>Identificación</p>
                    <div id="identificationVerificationUserProfile"></div>
                </div>
                <div class="profileBox_data_uploaded_verify">
                    <p>Comprobante de domicilio</p>
                    <div id="addressVerificationUserProfile"></div>
                </div>
                <div class="profileBox_data_uploaded_verify">
                    <p>Comprobante de estado de cuenta</p>
                    <div id="accountVerificationUserProfile"></div>
                </div>
            </div>
        </div>
    </div>
    {{/each}}

    <div class="changeRoleContainer">
        <div class="changeRoleContainer_box">
            <div class="changeRoleContainer_box_title">
                <h3>Cambio de role</h3>
            </div>
            <form id="toChangeRoleForm" enctype="multipart/form-data">
                <div class="changeRoleContainer_box_options">
                    <select name="changerole" id="optionToChangeRole">
                        <option value="USER">Cuenta tipo USER</option>
                        <option value="ADMIN">Cuenta tipo ADMIN</option>
                        <option value="PREMIUM">Cuenta tipo PREMIUM</option>
                    </select>
                </div>
                <div>
                    <button type="submit" class="btn" id="toChangeRoleProfileSubmit">
                        Enviar
                    </button>
                    <button type="button" class="btn" id="ChangeRoleProfileClose">
                        Cerrar
                    </button>
                </div>
            </form>
            <div id="loadingIndicator" style="display: none;">
                <img src="/IMG/loading.gif" alt="">
            </div>
        </div>
    </div>
</div>

<div class="changeRoleContainer">
    <div class="changeRoleContainer_box">
        <div>
            <p>¿Estás seguro que quieres eliminar a este usuario?</p>
        </div>
        <div>
            <button type="submit" class="btn" id="toChangeRoleProfileSubmit">
                Enviar
            </button>
            <button type="button" class="btn" id="ChangeRoleProfileClose">
                Cerrar
            </button>
        </div>
    </div>
</div>