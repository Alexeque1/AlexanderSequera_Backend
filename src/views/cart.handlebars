<div class="buttonAlert" id="buttonBack-cart">
    <p>Menu principal</p>
</div>

<div class="cartFullContainer">
    <div class="cartFullContainer_products">
        <div class="cartFullContainer_products_title">
            <h2>Carrito</h2>
        </div>
        <div class="cartFullContainer_products_list">
            {{#each results}}
            <div class="products_list_box">
                <div class="products_list_icons">
                    <div class="products_list_icons_click delete">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 24 24">
                            <path fill="red"
                                d="M19 4h-3.5l-1-1h-5l-1 1H5v2h14M6 19a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2V7H6v12Z" />
                        </svg>
                    </div>
                    <div class="products_list_icons_click plus">
                        <svg xmlns="http://www.w3.org/2000/svg" width="30" height="30" viewBox="0 0 24 24">
                            <g fill="none" fill-rule="evenodd">
                                <path
                                    d="M24 0v24H0V0h24ZM12.594 23.258l-.012.002l-.071.035l-.02.004l-.014-.004l-.071-.036c-.01-.003-.019 0-.024.006l-.004.01l-.017.428l.005.02l.01.013l.104.074l.015.004l.012-.004l.104-.074l.012-.016l.004-.017l-.017-.427c-.002-.01-.009-.017-.016-.018Zm.264-.113l-.014.002l-.184.093l-.01.01l-.003.011l.018.43l.005.012l.008.008l.201.092c.012.004.023 0 .029-.008l.004-.014l-.034-.614c-.003-.012-.01-.02-.02-.022Zm-.715.002a.023.023 0 0 0-.027.006l-.006.014l-.034.614c0 .012.007.02.017.024l.015-.002l.201-.093l.01-.008l.003-.011l.018-.43l-.003-.012l-.01-.01l-.184-.092Z" />
                                <path fill="green"
                                    d="M9 5a2 2 0 0 1 2-2h2a2 2 0 0 1 2 2v4h4a2 2 0 0 1 2 2v2a2 2 0 0 1-2 2h-4v4a2 2 0 0 1-2 2h-2a2 2 0 0 1-2-2v-4H5a2 2 0 0 1-2-2v-2a2 2 0 0 1 2-2h4V5Z" />
                            </g>
                        </svg>
                    </div>
                </div>
                <picture class="products_list_picture" id="products_list_picture">
                    <img class="products_list_image" src={{thumbnail}}>
                </picture>
                <div class='products_list_info'>
                    <h3> <span class="product_list_title"> {{title}} </span></h3>
                    <p class="product_list_rest"> {{price}} </p>
                    <p class="product_list_rest"> {{stock}} </p>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
    <div class="cartFullContainer_info">

    </div>
</div>

<script src="/js/chat.js"></script>
<script src="/js/cart.js"></script>
<script src="/js/index.js"></script>